{
  "name": "agentcommander-v1.1",
  "version": "1.1.0",
  "description": "AgentCommander V1.1 - God-Tier Multi-Agent Orchestrator with Redis coordination",
  "type": "module",
  "main": "dist/orchestrator/index.js",
  "scripts": {
    "dev": "bun --watch orchestrator/index.ts",
    "dev:agent": "bun --watch agent/worker.ts",
    "start": "bun orchestrator/index.ts",
    "start:agent": "bun agent/worker.ts",
    "build": "bun build orchestrator/index.ts --outdir=dist --target=bun",
    "type-check": "bun x tsc --noEmit",
    "lint": "bun x eslint . --cache",
    "lint:fix": "bun x eslint . --cache --fix",
    "format": "bun x prettier --write .",
    "format:check": "bun x prettier --check ."
  },
  "keywords": [
    "ai",
    "multi-agent",
    "orchestrator",
    "telegram",
    "redis",
    "claude"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.57",
    "express": "^5.2.1",
    "pg": "^8.13.3",
    "redis": "^4.7.0",
    "telegraf": "^4.16.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/express": "^5.0.5",
    "@types/node": "^22.0.0",
    "@types/pg": "^8.11.6",
    "@types/uuid": "^10.0.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "bun": ">=1.0.0"
  }
}
